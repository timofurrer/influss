{
  "manifest_version": 2,
  "name": "Influss Clipper",
  "version": "1.0.0",
  "description": "Minimalist website clipper to be consumed by your favorite RSS reader.",
  "permissions": [
    "contextMenus",
    "storage",
    "<all_urls>"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{f2313271-d6ed-4843-8157-00b56c01f40a}"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options.html"
  }
}
